@model FrontEnd.Models.Mercedes
@{
    ViewBag.Title = "Home";
}
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mercedes Benz</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"></script>
    <link href="@Url.Content("/Content/StyleSheet1.css")" rel="stylesheet">
    <script src="https://kit.fontawesome.com/32bd5643ee.js" crossorigin="anonymous"></script>


</head>

<body>

    @using (Html.BeginForm("Home", "WebApplication1.Controllers.MercedesController", FormMethod.Post))
    {
        <div id="carouselExampleCaptions" class="carousel slide">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="~/OIP (1).jpeg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>The new AMG GT roadster</h5>
                        <p>Shine in class.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/R (1).jpeg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Combining class with reliability and comfort</h5>
                        <p>Explore our C class models</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/OIP (2).jpeg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Looking for new adventures?</h5>
                        <p>CLS models can take you so far away!</p>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
 

        

        <div id="carouselExampleSlidesOnly" class="carousel slide" data-bs-ride="carousel" data-bs-interval="3000">
            <h1>Our Recommendations</h1>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="~/gwagon.jpg" class="d-block w-100" width="100%" height="100%" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="~/gle.jpg" class="d-block w-100" width="100%" height="100%" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="~/ecoupe.jpg" class="d-block w-100" width="100%" height="100%" alt="...">
                </div>
            </div>
        </div>
        

        <div class="cont-wrapper">

            <div class="container-class-models">
                <i class="fa-solid fa-download"></i>
                <div class="text-class-models">
                    <a href="https://www.mercedes-benz.com.eg/en/passengercars/buy/brochure-downloads.html" target="_blank">Download Brochures</a>
                </div>
            </div>

            <div class="container-class-models">
                <i class="fa-solid fa-map-location-dot"></i>
                <div class="text-class-models">
                    <a href="https://www.mercedes-benz.com.eg/en/passengercars/mercedes-benz-cars/dealer-locator.html" target="_blank">Find nearest dealerships</a>
                </div>
            </div>

            <div class="container-class-models">
                <i class="fa-solid fa-file-signature"></i>
                <div class="text-class-models">
                    <a href="https://www.mercedes-benz.com.eg/en/passengercars/services/service-contracts.html">Service Contracts</a>
                </div>
            </div>

        </div>


    }
</body>
</html>